{
  "global": {
    "settings": {
      "color": {
        "palette": [
          {
            "slug": "foreground",
            "color": "#1a1a2a"
          },
          {
            "slug": "background",
            "color": "#ffffff"
          }
        ]
      },
      "styles": {
        "color": {
          "background": "var( --wp--preset--color--background )",
          "text": "var( --wp--preset--color--foreground )"
        }
      }
    }
    },
    "core/paragraph": {
      "styles": {
        "color": {
          "link": "var( --wp--preset--color--primary )",
          "text": "var( --wp--preset--color--foreground )"
        },
        "typography": {
          "fontSize": "var( --wp--preset--font-size--normal )",
          "lineHeight": "var( --wp--preset--line-height--large )"
        }
      }
    },
